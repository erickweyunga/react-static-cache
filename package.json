{
  "name": "react-static-cache",
  "version": "1.0.4",
  "description": "React package for caching static assets using service workers",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.mjs",
    "build:types": "tsc --emitDeclarationOnly",
    "prepare": "npm run build"
  },
  "keywords": [
    "react",
    "service-worker",
    "cache",
    "static-assets"
  ],
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "esbuild": "^0.19.11",
    "rollup": "^4.9.5",
    "rollup-plugin-esbuild": "^6.1.0",
    "typescript": "^5.3.3"
  },
  "pnpm": {
    "overrides": {
      "glob": "^10.3.10"
    }
  },
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}
